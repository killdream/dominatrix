{
  "name": "dominatrix",
  "version": "0.2.1",
  "description": "Push brand new DOM structures into pleasant submission!",
  "main": "./lib/index.js",
  "devDependencies": {
    "LiveScript": "~1.1.1",
    "mocha": "~1.8.1",
    "es5-shim": "~2.0.5",
    "mocha-phantomjs": "~2.0.1",
    "browserify": "~2.4.3"
  },
  "scripts": {
    "cc": "./node_modules/.bin/lsc -o lib -c src/*.ls",
    "pretest": "npm run cc && ./node_modules/.bin/browserify -o test/browser/suite-bundle.js -e test/browser/suite.js",
    "test": "./node_modules/.bin/mocha-phantomjs test/browser/tests.html",
    "prepublish": "npm run cc"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/killdream/dominatrix.git"
  },
  "author": "Quildreen \"Sorella\" Motta <quildreen@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "calliope": {
    "output": "docs/api",
    "copyright": "Â© 2013 Quildreen \"Sorella\" Motta",
    "packages": [
      {
        "files": [
          "src/*.ls"
        ]
      }
    ]
  },
  "testling": {
    "files": "test/browser/suite.js",
    "preprocess": "npm run pretest",
    "browsers": {
      "ie": [
        8,
        9,
        10
      ],
      "firefox": [
        15,
        "nightly"
      ],
      "chrome": [
        21,
        22,
        "canary"
      ],
      "opera": [
        10,
        12,
        "next"
      ],
      "safari": [
        5.1
      ]
    },
    "harness": "mocha"
  }
}
