{
  "name": "dominatrix",
  "version": "1.0.0",
  "description": "Push brand new DOM structures into pleasant submission!",
  "main": "./lib/index.js",
  "devDependencies": {
    "LiveScript": "~1.1.1",
    "mocha": "~1.8.1",
